{
  "name": "@power-assert/rollup-plugin",
  "version": "0.0.0",
  "description": "power-assert rollup plugin",
  "type": "module",
  "main": "./dist/rollup-plugin-power-assert.mjs",
  "exports": {
    ".": "./dist/rollup-plugin-power-assert.mjs",
    "./package.json": "./package.json"
  },
  "dependencies": {
