{
  "name": "rollup-plugin-power-assert",
  "version": "0.0.0",
  "description": "power-assert rollup plugin",
  "author": {
    "name": "Takuto WADA",
    "email": "takuto.wada@gmail.com",
    "url": "https://github.com/twada"
  },
  "license": "MIT",
  "keywords": [
    "power-assert",
    "rollup-plugin"
  ],
  "type": "module",
  "main": "./dist/rollup-plugin-power-assert.mjs",
  "exports": {
    ".": "./dist/rollup-plugin-power-assert.mjs",
    "./package.json": "./package.json"
  },
  "files": [
    "src",
    "dist",
    "package.json"
  ],
  "dependencies": {
    "@power-assert/transpiler": "^0.3.0",
    "@power-assert/transpiler-core": "^0.3.0",
    "@rollup/pluginutils": "^5.1.0"
  },
  "peerDependencies": {
    "@power-assert/runtime": "^0.2.0"
  },
  "devDependencies": {
    "@types/estree": "^1.0.1",
    "rollup": "^4.2.0"
  },
  "scripts": {
    "example": "rollup --config rollup.config.power.mjs && node --test --enable-source-maps tmp/bowling.test.js",
    "build:clean": "rimraf dist && rm -f tsconfig.tsbuildinfo && rimraf tmp",
    "preversion": "rimraf dist/__tests__",
    "lint": "eslint src",
    "fix": "npm run lint:src -- --fix",
    "test": "node --no-warnings --test --test-reporter=spec 'dist/**/*test.mjs'"
  }
}
