{
  "name": "@power-assert/esm",
  "version": "0.0.0",
  "description": "power-assert esm loader",
  "main": "dist/esm-loader.mjs",
  "devDependencies": {},
  "scripts": {
    "build:clean": "rimraf dist && rm tsconfig.tsbuildinfo",
    "lint": "eslint src --ext .mts",
    "fix": "npm run lint:src -- --fix",
    "test:node": "node --no-warnings --test --test-reporter=spec '**/*test.mjs'",
    "test:prove": "prove --exec 'node --no-warnings --test --test-reporter=tap' -r dist --ext=test.mjs",
    "test": "npm run lint && npm run test:prove"
  },
  "keywords": [
    "power-assert"
  ],
  "author": "Takuto WADA",
  "license": "MIT",
  "dependencies": {
    "@power-assert/transpiler": "^0.0.0",
    "@power-assert/runtime": "^0.0.0"
  }
}
