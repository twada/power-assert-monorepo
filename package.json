{
  "name": "espower3",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "type": "module",
  "files": [
    "src",
    "dist",
    "package.json"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./runtime": {
      "import": {
        "types": "./dist/runtime/runtime.d.mts",
        "default": "./dist/runtime/runtime.mjs"
      },
      "default": "./dist/runtime/runtime.mjs"
    },
    "./transpiler": {
      "import": {
        "types": "./dist/transpiler/transpile-with-sourcemap.d.mts",
        "default": "./dist/transpiler/transpile-with-sourcemap.mjs"
      },
      "default": "./dist/transpiler/transpile-with-sourcemap.mjs"
    },
    "./esm": {
      "import": {
        "default": "./dist/esm/power-assert.mjs"
      },
      "default": "./dist/esm/power-assert.mjs"
    }
  },
  "dependencies": {
    "acorn": "^8.8.0",
    "astring": "^1.8.4",
    "convert-source-map": "^2.0.0",
    "easta": "^7.0.0",
    "emoji-regex": "^10.2.1",
    "estraverse": "^5.3.0",
    "esutils": "^2.0.0",
    "multi-stage-sourcemap": "^0.3.1",
    "source-map": "^0.7.4"
  },
  "devDependencies": {
    "@types/convert-source-map": "^2.0.0",
    "@types/escodegen": "^0.0.8",
    "@types/estraverse": "^5.1.2",
    "@types/estree": "^1.0.1",
    "@types/esutils": "^2.0.0",
    "@types/multi-stage-sourcemap": "^0.3.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "escodegen": "^2.0.0",
    "eslint": "^8.39.0",
    "eslint-config-semistandard": "^17.0.0",
    "espurify": "^3.0.0",
    "rimraf": "^5.0.0",
    "rollup": "^3.0.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-node-externals": "^6.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "scripts": {
    "example": "npm run build && npm run power:example",
    "power": "node --enable-source-maps --no-warnings --loader espower3/esm --test --",
    "power:example": "node --enable-source-maps --no-warnings --loader espower3/esm --test examples/bowling_test.mjs",
    "ts-node:power": "TS_NODE_PROJECT='./tsconfig.tsnode.json' node --enable-source-maps --no-warnings --loader ts-node/esm --loader espower3/esm --test --",
    "ts-node:power:example": "npm run build && TS_NODE_PROJECT='./tsconfig.tsnode.json' node --enable-source-maps --no-warnings --loader ts-node/esm --loader espower3/esm --test examples/spike/spike_test.mts",
    "preversion": "npm run build && npm test",
    "build": "npm run build:clean && npm run build:mjs",
    "build:clean": "rimraf dist",
    "build:mjs": "tsc",
    "dist": "npm run build:cjs",
    "build:cjs": "rollup -c",
    "lint": "eslint src --ext .mts",
    "fix": "npm run lint:src -- --fix",
    "test:ts": "TS_NODE_PROJECT='./tsconfig.tsnode.json' node --test --enable-source-maps --no-warnings --loader ts-node/esm --",
    "test:dot": "npm run test:ts --test-reporter=dot --",
    "test:spec": "npm run test:ts --test-reporter=spec --",
    "test:prove": "prove --exec 'node --no-warnings --test --test-reporter=tap' -r dist --ext=test.mjs",
    "test": "npm run lint && npm run build && npm run test:prove"
  }
}
