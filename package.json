{
  "name": "espower3",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "keywords": [],
  "author": "",
  "license": "MIT",
  "type": "module",
  "files": [
    "src",
    "dist/src",
    "package.json"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./runtime": {
      "import": {
        "types": "./dist/src/runtime/runtime.d.mts",
        "default": "./dist/src/runtime/runtime.mjs"
      },
      "default": "./dist/src/runtime/runtime.mjs"
    },
    "./transpiler": {
      "import": {
        "types": "./dist/src/transpiler/transpile-with-sourcemap.d.mts",
        "default": "./dist/src/transpiler/transpile-with-sourcemap.mjs"
      },
      "default": "./dist/src/transpiler/transpile-with-sourcemap.mjs"
    },
    "./esm": {
      "import": {
        "default": "./dist/src/esm/power-assert.mjs"
      },
      "default": "./dist/src/esm/power-assert.mjs"
    }
  },
  "dependencies": {
    "acorn": "^8.8.0",
    "astring": "^1.8.4",
    "convert-source-map": "^2.0.0",
    "easta": "^7.0.0",
    "emoji-regex": "^10.2.1",
    "estraverse": "^5.3.0",
    "esutils": "^2.0.0",
    "multi-stage-sourcemap": "^0.3.1",
    "source-map": "^0.7.4"
  },
  "devDependencies": {
    "@types/convert-source-map": "^2.0.0",
    "@types/estraverse": "^5.1.2",
    "@types/estree": "^1.0.1",
    "@types/esutils": "^2.0.0",
    "@types/multi-stage-sourcemap": "^0.3.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "escodegen": "^2.0.0",
    "eslint": "^8.39.0",
    "eslint-config-semistandard": "^17.0.0",
    "espurify": "^3.0.0",
    "rimraf": "^5.0.0",
    "rollup": "^3.0.0",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-node-externals": "^6.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "scripts": {
    "power": "npm run build && node --enable-source-maps --no-warnings --loader espower3/esm --test --",
    "ts-node:power": "npm run build && TS_NODE_PROJECT='./tsconfig.tsnode.json' node --enable-source-maps --no-warnings --loader ts-node/esm --loader espower3/esm --test --",
    "ts-node:power:nomap": "npm run build && TS_NODE_PROJECT='./tsconfig.tsnode.json' node --no-warnings --loader ts-node/esm --loader espower3/esm --test --",
    "ts-node": "TS_NODE_PROJECT='./tsconfig.tsnode.json' node --enable-source-maps --no-warnings --loader ts-node/esm --test test/spike/spike_test.mts",
    "example": "node --enable-source-maps --no-warnings --experimental-loader espower3/esm --test examples/bowling_test.mjs",
    "multi-sm": "npm run build && node --enable-source-maps --no-warnings --experimental-loader espower3/esm --test dist/test/spike/spike_test.mjs",
    "preversion": "npm run build && npm test",
    "build": "npm run build:clean && npm run build:mjs && npm run build:cjs",
    "build:clean": "rimraf dist",
    "build:mjs": "tsc",
    "build:cjs": "rollup -c",
    "lint": "npm run lint:src && npm run lint:test",
    "lint:src": "eslint src --ext .mts",
    "lint:test": "eslint test/*test.mjs test/**/*test.mjs --ext .mjs",
    "fix": "npm run fix:src && npm run fix:test",
    "fix:src": "npm run lint:src -- --fix",
    "fix:test": "npm run lint:test -- --fix",
    "test:default": "node --no-warnings --test test",
    "test:dot": "node --no-warnings --test --test-reporter=dot test",
    "test:spec": "node --no-warnings --test --test-reporter=spec test",
    "test:prove": "prove --exec 'node --no-warnings --test --test-reporter=tap' -r test --ext=test.mjs",
    "test:unit": "npm run test:dot",
    "test": "npm run lint && npm run build && npm run test:default"
  }
}
